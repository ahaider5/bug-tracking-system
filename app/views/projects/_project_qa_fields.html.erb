<fieldset>
  <%= f.select :user_id, User.where(user_type: 1).collect {|p| [ p.name, p.id ] }, {prompt: 'Select User'}, { class: 'form-control', style: 'width: 300px; display: initial;' } %>
  <%= f.hidden_field :_destroy %>
  <%= link_to "Remove", '#', { class: "remove_fields" } %>
</fieldset>
